<xsd:schema elementFormDefault="qualified" targetNamespace="http://xmlns.oracle.com/apps/hcm/people/roles/userDetailsServiceV2/"
     sdoJava:package="oracle.apps.hcm.people.roles.userDetailsServiceV2" xmlns:ns0="http://xmlns.oracle.com/adf/svc/types/"
     xmlns:sdoJava="commonj.sdo/java" xmlns:sdoXML="commonj.sdo/xml" xmlns="http://xmlns.oracle.com/apps/hcm/people/roles/userDetailsServiceV2/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:annotation>
        <xsd:documentation xmlns:oer="http://xmlns.oracle.com/oer">
            <name>User Work Relationship Details Version 2</name>
            <description>Highest level of the employment model that exists for every
                worker residing within a legal employer. This layer holds
                information regarding the type of relationship that exists
                between the person and the enterprise.</description>
            <oer:category name="lba">
                <value>HcmTop-HcmTop</value>
                <value>HcmTop-HcmTop : HcmEmploymentTop-Employment</value>
                <value>HcmTop-HcmTop : HcmEmploymentTop-Employment :
                    HcmEmploymentCore-Employment</value>
            </oer:category>
            <oer:category name="lbo">
                <value>HCM-HCM</value>
                <value>HCM-HCM : PER-Global Human Resources</value>
                <value>HCM-HCM : PER-Global Human Resources : PER_WORKER-Worker</value>
            </oer:category>
            <oer:attributes>
                <oer:attribute description="Identifier of the person." name="PersonId"/>
                <oer:attribute description="Identifier of the business unit." name="BusinessUnitId"/>
                <oer:attribute description="Name of the business unit." name="BusinessUnitName"/>
                <oer:attribute description="Name of the legal entity." name="LegalEntityName"/>
                <oer:attribute description="Identifier of the person type." name="PersonTypeId"/>
                <oer:attribute description="System person type." name="SystemPersonType"/>
                <oer:attribute description="User person type." name="UserPersonType"/>
                <oer:attribute description="Identifier of the work relationship." name="PeriodOfServiceId"/>
                <oer:attribute description="Hire date." name="HireDate"/>
                <oer:attribute description="Worker number." name="WorkerNumber"/>
                <oer:attribute description="Identifier of the assignment status type." name="AssignmentStatusTypeId"/>
                <oer:attribute description="User status." name="UserStatus"/>
                <oer:attribute description="Isentifier of the assignment." name="AssignmentId"/>
                <oer:attribute description="Assignment name." name="AssignmentName"/>
                <oer:attribute description="Assignment number." name="AssignmentNumber"/>
                <oer:attribute description="Assignment status type." name="AssignmentStatusType"/>
                <oer:attribute description="Assignment type." name="AssignmentType"/>
                <oer:attribute description="Primary flag." name="PrimaryFlag"/>
                <oer:attribute description="Identifier of the assignment supervisor." name="AssignmentSupervisorId"/>
                <oer:attribute description="Identifier of the manager." name="ManagerId"/>
                <oer:attribute description="Manager name." name="ManagerName"/>
                <oer:attribute description="Manager user name." name="ManagerUsername"/>
                <oer:attribute description="Identifier of the job." name="JobId"/>
                <oer:attribute description="Job name." name="JobName"/>
                <oer:attribute description="Job code." name="JobCode"/>
                <oer:attribute description="Identifier of the reference set." name="JobSetId"/>
                <oer:attribute description="Identifier of the department." name="DepartmentId"/>
                <oer:attribute description="Name of the department." name="DepartmentName"/>
                <oer:attribute description="Identifier of the department set." name="DepartmentSetId"/>
                <oer:attribute description="Identifier of the position." name="PositionId"/>
                <oer:attribute description="Name of the position." name="PositionName"/>
                <oer:attribute description="Code of the position." name="PositionCode"/>
                <oer:attribute description="Identifier of the grade." name="GradeId"/>
                <oer:attribute description="Name of the grade." name="GradeName"/>
                <oer:attribute description="Code of the grade." name="GradeCode"/>
                <oer:attribute description="Identifier of the grade set." name="GradeSetId"/>
                <oer:attribute description="Identifier of the location." name="LocationId"/>
                <oer:attribute description="Name of the location." name="LocationName"/>
                <oer:attribute description="Code of the location." name="LocationCode"/>
                <oer:attribute description="Identifier of the location set." name="LocationSetId"/>
                <oer:attribute description="Location building." name="LocationBuilding"/>
                <oer:attribute description="Location floor number." name="LocationFloorNumber"/>
                <oer:attribute description="Location address line 1." name="LocationAddressLine1"/>
                <oer:attribute description="Location address line 2." name="LocationAddressLine2"/>
                <oer:attribute description="Location address line 3." name="LocationAddressLine3"/>
                <oer:attribute description="Location address line 4." name="LocationAddressLine4"/>
                <oer:attribute description="Location region 1." name="LocationRegion1"/>
                <oer:attribute description="Location region 2." name="LocationRegion2"/>
                <oer:attribute description="Location region 3." name="LocationRegion3"/>
                <oer:attribute description="Location town or city." name="LocationTownOrCity"/>
                <oer:attribute description="Location postal code." name="LocationPostalCode"/>
                <oer:attribute description="Location country." name="LocationCountry"/>
                <oer:attribute description="Location long postal code." name="LocationLongPostalCode"/>
                <oer:attribute description="Location time zone code." name="LocationTimezoneCode"/>
                <oer:attribute description="Location address." name="LocationAddress"/>
            </oer:attributes>
        </xsd:documentation>
    </xsd:annotation>
    <xsd:import namespace="http://xmlns.oracle.com/adf/svc/types/" schemaLocation="BC4JService.xsd"/>
    <xsd:import namespace="commonj.sdo/java" schemaLocation="sdoJava.xsd"/>
    <xsd:import namespace="commonj.sdo/xml" schemaLocation="sdoXML.xsd"/>
