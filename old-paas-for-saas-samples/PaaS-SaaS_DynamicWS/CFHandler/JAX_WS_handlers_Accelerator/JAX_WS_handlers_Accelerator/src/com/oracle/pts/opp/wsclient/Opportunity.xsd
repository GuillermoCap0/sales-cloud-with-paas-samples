<xsd:schema elementFormDefault="qualified" targetNamespace="http://xmlns.oracle.com/apps/sales/opptyMgmt/opportunities/opportunityService/"
     sdoJava:package="oracle.apps.sales.opptyMgmt.opportunities.opportunityService" xmlns:ns0="http://xmlns.oracle.com/adf/svc/types/"
     xmlns:ns1="http://xmlns.oracle.com/apps/sales/opptyMgmt/revenues/revenueService/" xmlns:ns2="http://xmlns.oracle.com/apps/crmCommon/notes/noteService"
     xmlns:ns3="http://xmlns.oracle.com/oracle/apps/sales/opptyMgmt/revenues/revenueService/"
     xmlns:ns4="http://xmlns.oracle.com/apps/crmCommon/activities/activitiesService/" xmlns:sdo="commonj.sdo"
     xmlns:sdoJava="commonj.sdo/java" xmlns:sdoXML="commonj.sdo/xml" xmlns="http://xmlns.oracle.com/apps/sales/opptyMgmt/opportunities/opportunityService/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
        <xsd:documentation xmlns:oer="http://xmlns.oracle.com/oer" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <name>Opportunity</name>
            <description>An opportunity is used by a sales organization to track information about a potential sale such as the sales account, key contacts, product interests, and potential revenues.</description>
            <oer:attributes xmlns:oer="http://xmlns.oracle.com/oer">
                <oer:attribute description="Date when the budget will be available." name="BudgetAvailableDate"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Indicates if the opportunity sales account has its budget approved for the potential purchase."
                     name="BudgetedFlag" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Identifier of the business unit to which this opportunity belongs."
                     name="PrimaryOrganizationId" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Indicates if this opportunity has a key internal sponsor."
                     name="ChampionFlag" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Disconnected Mobile: this value is used to guarantee the uniqueness of the row when duplicates are created in different databases (i.e. mobile databases or the server)."
                     name="ConflictId" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Who column: indicates the user who created the row." name="CreatedBy"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Who column: indicates the date and time of the creation of the row."
                     name="CreationDate" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The currency the opportunity amount is based on." name="CurrencyCode"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Sales method identifier for this opportunity. This indicates the sales process used."
                     name="SalesMethodId" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Identifier of the sales stage of the opportunity." name="SalesStageId"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Customer account associated with the opportunity." name="CustomerAccountId"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Indicates how long it may take to close the deal. This field refers to the lookup type MOO_DEAL_HORIZON."
                     name="DealHorizonCode" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Indicates the decision level at the opportunity sales account to commit. This field refers to the lookup type MOO_DECISION_LEVEL."
                     name="DecisionLevelCode" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Description of the opportunity." name="Description" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Who column: indicates the date and time of the last update of the row."
                     name="LastUpdateDate" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Who column: indicates the user who last updated the row."
                     name="LastUpdatedBy" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Who column: indicates the session login associated to the user who last updated the row."
                     name="LastUpdateLogin" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Name of the opportunity." name="Name" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Used to implement optimistic locking. This number is incremented every time that the row is updated. The number is compared at the start and end of a transaction to detect whether another session has updated the row since it was queried."
                     name="ObjectVersionNumber" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Opportunity identifier." name="OptyId" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Opportunity number." name="OptyNumber" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Identifier of the opportunity owner." name="OwnerResourcePartyId"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Identifier of the primary competitor of this opportunity."
                     name="PrimaryCompetitorId" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Identifier of the primary contact." name="KeyContactId"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Win/Loss reason of the opportunity. Valid values from the MOO_WIN_LOSS_REASON lookup type."
                     name="ReasonWonLostCode" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Indicates the risk level for this opportunity. This field refers to the lookup type MOO_RISK_LEVEL."
                     name="RiskLevelCode" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Status of the opportunity. Valid values from the MOO_OPTY_STATUS lookup type."
                     name="StatusCode" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Indicates the level of the strategic value to work with the opportunity sales account. This field refers to the lookup type MOO_STRATEGIC_VALUE."
                     name="StrategicLevelCode" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Identifier for the summary revenue line for the opportunity. The summary revenue line stores the total revenue amount of an opportunity."
                     name="PrimaryRevenueId" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Party identifier of the opportunity sales account." name="TargetPartyId"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Disconnected Mobile: indicates the date and time of the last update of the row. This value is different from LAST_UPDATE_DATE if the update originally happened in a different database (i.e. a different mobile database or the server)."
                     name="UserLastUpdateDate" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Name of the sales account." name="TargetPartyName" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="D-U-N-S number of the sales account." name="DUNSNumber"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Sales method of the opportunity." name="SalesMethod" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Object version number of the sales method associated to the opportunity."
                     name="SalesMethodEOObjVerNumber" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Sales stage of the opportunity." name="SalesStage" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Sales objective of the opportunity." name="DescriptionText"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Average days an opportunity remains in the current sales stage."
                     name="AverageDaysAtStage" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Maximum days the opportunity can be in the sales stage prior to considering it stalled."
                     name="MaximumDaysInStage" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Phase of the opportunity sales stage." name="PhaseCd" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Quota factor of the opportunity sales stage." name="QuotaFactor"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Recommended win probability for an opportunity in the sales stage"
                     name="RcmndWinProb" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Default status for the opportunity sales stage." name="StageStatusCd"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Order of the opportunity&apos;s sales stage within the sales method."
                     name="StgOrder" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Object version number of the sales stage associated to the opportunity."
                     name="SalesStageEOObjVerNumber" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Estimated close date for the opportunity." name="EffectiveDate"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Revenue amount." name="Revenue" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="descoped attribute, do not use." name="RevenueType" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Revenue identifier." name="RevnId" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="descoped attribute, do not use." name="EmployeesTotal" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="descoped attribute, do not use." name="CurrentFyPotentialRevenue"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Win probability percent." name="WinProb" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Used to implement optimistic locking. This number is incremented every time that the row is updated. The number is compared at the start and end of a transaction to detect whether another session has updated the row since it was queried."
                     name="ObjectVersionNumber1" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Name of the primary contact." name="PrimaryContactPartyName"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Party identifier of the primary contact." name="PrimaryContactPartyId"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Formatted phone number for the primary contact on the opportunity."
                     name="PrimaryContactFormattedPhoneNumber" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="E-mail address of the primary contact." name="PrimaryContactEmailAddress"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Account number of the opportunity sales account." name="AccountNumber"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Name of the primary competitor." name="PartyName" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="descoped attribute, do not use." name="LocationId" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="descoped attribute, do not use." name="FormattedAddress"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="descoped attribute, do not use." name="City" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="descoped attribute, do not use." name="Country" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="descoped attribute, do not use." name="PostalCode" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="descoped attribute, do not use." name="State" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Budget amount from the sales account." name="BdgtAmount"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Comments about this opportunity." name="Comments" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Name of the opportunity owner." name="PartyName1" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Primary marketing source or campaign that generated this opportunity."
                     name="PrSrcNumber" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Worst case amount." name="DownsideAmount" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Best case amount." name="UpsideAmount" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="E-mail address of the opportunity owner." name="EmailAddress"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The date when the opportunity is last assigned." name="LastAssignedDate"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Status category of the opportunity." name="LookupCategory"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Lookup value identifier for the opportunity status." name="LookupValuesId"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Replicates WHO column creation_date for functional use."
                     name="OptyCreationDate" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Expected revenue amount." name="ExpectAmount" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Preferred functional currency of the opportunity sales account."
                     name="PreferredFunctionalCurrency" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="This is a derived column. For Organization party, it is the concatenation of unique_name_alias and the suffix. For Person party, this is the concatenation of person_name and suffix."
                     name="PartyUniqueName" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Relationship identifier of the primary contact on the opportunity."
                     name="PrConRelationshipId" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Replicates WHO column CREATED_BY for functional use."
                     name="OptyCreatedBy" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="This rate is used for the conversion of the revenue amount to the CRM common currency to facilitate the computation of the revenue forecast metrics. This is the amount that one currency can be exchanged for, at a specific point of time."
                     name="CrmConversionRate" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Opportunity revenue amount converted to the corporate currency."
                     name="CrmRevenue" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Indicates whether the sales method on the opportunity is revenue line set enabled."
                     name="RevnLineSetEnabledFlag" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The party type can only be Person, Organization, or Group."
                     name="PartyType" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Replicates WHO column last_update_date for functional use."
                     name="OptyLastUpdateDate" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Replicates WHO column last_updated_by for functional use."
                     name="OptyLastUpdatedBy" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Indicates the sales channel for the opportunity." name="SalesChannelCd"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The line of business that is responsible for the opportunity."
                     name="LineOfBusinessCode" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Indicates if the contact has specified a phone preference."
                     name="PhonePreferenceExistsFlag" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Unique name of the primary opportunity competitor" name="PartyUniqueName1"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Name of the business unit to which this opportunity belongs."
                     name="PrimaryOrganizationName" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Identifier of the opportunity sales account." name="SalesAccountId"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Name of the sales account." name="SalesAccountUniqueName"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The First Name of the Target Party for the Opportunity."
                     name="ConsumerFirstName" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The Last Name of the Target Party for the Opportunity."
                     name="ConsumerLastName" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The First Name of the Primary Contact for the Opportunity."
                     name="PrimaryContactFirstName" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The Last Name of the Primary Contact for the Opportunity."
                     name="PrimaryContactLastName" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The timestamp when the opportunity is last assigned."
                     name="LastAssignedTimestamp" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Contacts associated with the opportunity." name="OpportunityContact"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Revenue items on the opportunity." name="ChildRevenue" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Notes associated with the opportunity." name="Note" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="References associated with the opportunity." name="OpportunityReference"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="All competitors associated with the opportunity, including both primary and non-primary competitors."
                     name="OpportunityCompetitor2" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Non-primary competitors associated with the opportunity."
                     name="OpportunityCompetitor3" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Resources on the opportunity team." name="OpportunityResource"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Marketing sources associated to the opportunity." name="OpportunitySource1"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Leads associated to the opportunity." name="OpportunityLead"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Marketing responses associated with the opportunity."
                     name="OpportunityResponse" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Territories assigned to the opportunity." name="RevenueTerritory"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Partners associated to the opportunity." name="RevenuePartnerPrimary"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Revenue line set associated with the opportunity." name="RevenueLineSet"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Category summary revenue for revenue line set enabled opportunity"
                     name="CategorySummaryRevenue" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="Appointments associated with the opportunity." name="Appointment"
                     xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The revenue or nonrevenue credit allocated to resources who have contributed to the selling effort for the opportunity."
                     name="SplitRevenue" xmlns:oer="http://xmlns.oracle.com/oer"/>
                <oer:attribute description="The revenues that span over a period of time sourced from the same product on the opportunity."
                     name="RecurringRevenue" xmlns:oer="http://xmlns.oracle.com/oer"/>
            </oer:attributes>
        </xsd:documentation>
    </xsd:annotation>
    <xsd:import namespace="http://xmlns.oracle.com/adf/svc/types/" schemaLocation="BC4JService.xsd"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="http://xmlns.oracle.com/apps/sales/opptyMgmt/revenues/revenueService/"
         schemaLocation="Revenue.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="http://xmlns.oracle.com/apps/crmCommon/notes/noteService" schemaLocation="Note.xsd"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="http://xmlns.oracle.com/apps/sales/opptyMgmt/revenues/revenueService/"
         schemaLocation="RevenueTerritory.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="http://xmlns.oracle.com/apps/sales/opptyMgmt/revenues/revenueService/"
         schemaLocation="RevenuePartner.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="http://xmlns.oracle.com/oracle/apps/sales/opptyMgmt/revenues/revenueService/"
         schemaLocation="RevenueLineSet.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="http://xmlns.oracle.com/oracle/apps/sales/opptyMgmt/revenues/revenueService/"
         schemaLocation="CategorySummaryRevenue.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="http://xmlns.oracle.com/apps/crmCommon/activities/activitiesService/"
         schemaLocation="TransientAppointment.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="http://xmlns.oracle.com/apps/sales/opptyMgmt/revenues/revenueService/"
         schemaLocation="SplitRevenue.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="http://xmlns.oracle.com/apps/sales/opptyMgmt/revenues/revenueService/"
         schemaLocation="RecurringRevenue.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="commonj.sdo/java" schemaLocation="sdoJava.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="commonj.sdo/xml" schemaLocation="sdoXML.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:import namespace="commonj.sdo" schemaLocation="sdoModel.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:include schemaLocation="OpportunityContact.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:include schemaLocation="OpportunityReference.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:include schemaLocation="OpportunityCompetitor.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:include schemaLocation="OpportunityResource.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:include schemaLocation="OpportunitySource.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:include schemaLocation="OpportunityLead.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:include schemaLocation="OpportunityResponse.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
    <xsd:complexType name="Opportunity" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
        <xsd:annotation xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:appinfo source="http://xmlns.oracle.com/adf/svc/metadata/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                <key xmlns="http://xmlns.oracle.com/adf/svc/metadata/">
                    <attribute>OptyId</attribute>
                </key>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:element minOccurs="0" name="BudgetAvailableDate" nillable="true" type="ns0:date-Date" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="BudgetedFlag" nillable="true" type="xsd:boolean" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="PrimaryOrganizationId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"
                 xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="ChampionFlag" type="xsd:boolean" sdoXML:dataType="sdoJava:BooleanObject"
                 xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="ConflictId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"
                 xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="CreatedBy" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="CreationDate" type="ns0:dateTime-Timestamp" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="CurrencyCode" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="SalesMethodId" nillable="true" type="xsd:long" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="SalesStageId" nillable="true" type="xsd:long" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="CustomerAccountId" nillable="true" type="xsd:long" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="DealHorizonCode" nillable="true" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="DecisionLevelCode" nillable="true" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="Description" nillable="true" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="LastUpdateDate" type="ns0:dateTime-Timestamp" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="LastUpdatedBy" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
            <xsd:element minOccurs="0" name="LastUpdateLogin" nillable="true" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema"/>
