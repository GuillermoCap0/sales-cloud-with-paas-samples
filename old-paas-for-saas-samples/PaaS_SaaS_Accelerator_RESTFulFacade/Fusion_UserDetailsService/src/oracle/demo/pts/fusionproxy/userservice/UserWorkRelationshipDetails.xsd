            <xsd:element minOccurs="0" name="SystemPersonType" type="xsd:string"/>
            <xsd:element minOccurs="0" name="UserPersonType" type="xsd:string"/>
            <xsd:element minOccurs="0" name="PeriodOfServiceId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="HireDate" type="ns0:date-Date"/>
            <xsd:element minOccurs="0" name="WorkerNumber" type="xsd:string"/>
            <xsd:element minOccurs="0" name="AssignmentStatusTypeId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="UserStatus" type="xsd:string"/>
            <xsd:element minOccurs="0" name="AssignmentId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="AssignmentName" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="AssignmentNumber" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="AssignmentStatusType" type="xsd:string"/>
            <xsd:element minOccurs="0" name="AssignmentType" type="xsd:string"/>
            <xsd:element minOccurs="0" name="PrimaryFlag" type="xsd:boolean" sdoXML:dataType="sdoJava:BooleanObject"/>
            <xsd:element minOccurs="0" name="AssignmentSupervisorId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="ManagerId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="ManagerName" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="ManagerUsername" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="JobId" nillable="true" type="xsd:long"/>
            <xsd:element minOccurs="0" name="JobName" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="JobCode" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="JobSetId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="DepartmentId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="DepartmentName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="DepartmentSetId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="PositionId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="PositionName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="PositionCode" type="xsd:string"/>
            <xsd:element minOccurs="0" name="GradeId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="GradeName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="GradeCode" type="xsd:string"/>
            <xsd:element minOccurs="0" name="GradeSetId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="LocationId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="LocationName" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationCode" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationSetId" type="xsd:long" sdoXML:dataType="sdoJava:LongObject"/>
            <xsd:element minOccurs="0" name="LocationBuilding" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationFloorNumber" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationAddressLine1" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationAddressLine2" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationAddressLine3" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationAddressLine4" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationRegion1" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationRegion2" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationRegion3" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationTownOrCity" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationPostalCode" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationCountry" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationLongPostalCode" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationTimezoneCode" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="LocationAddress" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="UserWorkRelationshipDetailsResult">
        <xsd:complexContent>
            <xsd:extension base="ns0:MethodResult">
                <xsd:sequence>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="Value" type="UserWorkRelationshipDetails"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="userWorkRelationshipDetailsResult" type="UserWorkRelationshipDetailsResult"/>
    <xsd:element name="userWorkRelationshipDetails" type="UserWorkRelationshipDetails"/>
</xsd:schema>
