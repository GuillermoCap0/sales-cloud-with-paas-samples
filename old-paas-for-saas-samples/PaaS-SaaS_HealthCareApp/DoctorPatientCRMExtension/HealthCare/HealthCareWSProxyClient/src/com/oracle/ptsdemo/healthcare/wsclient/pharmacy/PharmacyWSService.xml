<?xml version = '1.0' encoding = 'UTF-8'?>
<oracle-webservice-clients xmlns:ns2="http://java.sun.com/xml/ns/javaee">
   <webservice-client>
      <service-qname namespaceURI="http://pharmacy.webservice.pts.oracle.com/"
                     localpart="PharmacyWSService"/>
      <port-info>
         <wsdl-port namespaceURI="http://pharmacy.webservice.pts.oracle.com/"
                    localpart="PharmacyWSPort"/>
         <runtime/>
         <policy-references>
            <policy-reference category="security"
                              uri="oracle/wss_username_token_client_policy"/>
         </policy-references>
         <operations>
            <operation name="createMedication"/>
            <operation name="isMedicationReadyToPickup"/>
            <operation name="requestOrderStatus"/>
            <operation name="setMedicationReadyToPickup"/>
            <operation name="loadMedicationListFromHealthCare"/>
            <operation name="loadMedicationListForHealthCare"/>
            <operation name="loadHealthCareProviderList"/>
            <operation name="createPrescription"/>
         </operations>
      </port-info>
   </webservice-client>
</oracle-webservice-clients>
